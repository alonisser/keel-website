
<div class="content with-sidebar blog post">  
  <h1><%- page.title %></h1>    
  <% site.posts.sort('date', -1).each(function (post) { %>
        <li>
          <a href="/<%- post.path %>" class="sidebar-link<%- page.title === post.title ? ' current' : '' %>"><%- post.title %></a> <em> <%- post.date.format('MMM D[,] YYYY') %> </em>
          <br/> 
          <%- partial('partials/post/tag', {tags: post.tags})%>
        </li>
      <% }) %>
</div>
